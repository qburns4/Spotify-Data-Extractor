   "source": [
    "# **CODE BELOW IS MEANT TO BE RERUN/AUTOMATED**"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 209,
   "id": "adab13ce-b5cd-4036-b2d4-90a21aaee452",
   "metadata": {},
   "outputs": [],
   "source": [
    "sp_update = spotipy.Spotify(auth_manager=SpotifyOAuth(\n",
    "    client_id=CLIENT_ID,\n",
    "    client_secret=CLIENT_SECRET,\n",
    "    redirect_uri=REDIRECT_URI,\n",
    "    scope=scope\n",
    "))\n",
    "\n",
    "# Return last 50 recently played songs\n",
    "result_update = sp_update.current_user_recently_played(limit=50)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 210,
   "id": "051d9c03-e28a-42a2-af75-73e86c85302d",
   "metadata": {},
   "outputs": [],
   "source": [
    "#retakes most recent spotify data, and saves it in a new df\n",
    "data_2 = []\n",
    "for item in result_update['items']:\n",
    "    track = item['track']\n",
    "    data_2.append({\n",
    "        \"song\": track['name'],\n",
    "        \"artist\": track['artists'][0]['name'],\n",
    "        \"album\": track['album']['name'],\n",
    "        \"played_at\": item['played_at']\n",
    "    })\n",
    "\n",
    "# Convert to DataFrame\n",
    "df_new = pd.DataFrame(data_2)\n",
    "df_new['played_at'] = pd.to_datetime(df_new['played_at'])\n",
    "df_new['played_at'] = df_new['played_at'].apply(to_string)\n",
    "df_new = df_new.sort_values(by='played_at')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 228,
   "id": "8d207d14-3b00-421a-b0db-aeddd2ffba08",
   "metadata": {},
   "outputs": [],
   "source": [
    "#function to update dataframe with the newest values appended at the end of the dataframe\n",
    "def updated_df(old_df, new_df):\n",
    "    count = 0\n",
    "    #saves the latest entry in the old df to last to compare to new df\n",
    "    last = old_df['played_at'].tolist()[-1]\n",
    "\n",
    "    #gets the index in which the old df overlap ends in the new df\n",
    "    try:\n",
    "        \n",
    "        while last != new_df['played_at'].tolist()[count]:\n",
    "            count += 1\n",
    "\n",
    "    #incase I listened to more than 50 songs within the last update, it'll just append those songs\n",
    "    except IndexError:\n",
    "        updated = pd.concat([old_df, new_df])\n",
    "        return updated\n",
    "        \n",
    "    #increase count by 1 to start at new values, save new values to a variable, and append new values to old df\n",
    "    else:\n",
    "        count = count + 1\n",
    "        new_values = new_df.iloc[count:]\n",
    "        updated = pd.concat([old_df, new_values])\n",
    "        return updated\n"
   ]
  },
